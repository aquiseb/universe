{
	"presets": ["next/babel"],
	"plugins": [
		"relay",
		["styled-components", { "ssr": true }],
		[
			"import",
			{
				"libraryName": "antd",
				"style": "css"
			}
		],
		[
			"module-resolver",
			{
				"root": ["."],
				"alias": {
					"@comps": "./components",
					"@ui": "./ui"
				}
			}
		]
	]
}
